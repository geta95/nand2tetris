/**
 * 16-bit Not:
 * for i=0..15: out[i] = not in[i]
 */

CHIP Not16 {
    IN in[16];
    OUT out[16];

    PARTS:
    // Put your code here:
        Not (in[0], out=out[0]);
        Not (in[1], out=out[0]);
        Not (in[2], out=out[0]);
        Not (in[3], out=out[0]);
        Not (in[4], out=out[0]);
        Not (in[5], out=out[0]);
        Not (in[6], out=out[0]);
        Not (in[7], out=out[0]);
        Not (in[8], out=out[0]);
        Not (in[9], out=out[0]);
        Not (in[10], out=out[0]);
        Not (in[11], out=out[0]);
        Not (in[12], out=out[0]);
        Not (in[13], out=out[0]);
        Not (in[14], out=out[0]);
        Not (in[15], out=out[0]);
}
